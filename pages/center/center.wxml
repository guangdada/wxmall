<view class="m-ucenter g-row greybg">
  <view class="m-uc-top">
    <view class="m-profile">
      <image class="avatar" src="{{ userInfo !=null ? userInfo.avatarUrl : '../../images/header.png' }}" alt=""></image>
      <view class="info" bindtap="login">
        <block wx:if="{{userInfo != null}}">
          <view class="nickname">
            {{ userInfo.nickName }}
            <!-- 未登录 -->
          </view>
          <view class="membershipLevel">
            <text class="memberTitle">您好</text>
          </view>
        </block>
        <block wx:if="{{userInfo == null}}">
          <view class="nickname">
            未登录
          </view>
          <view class="membershipLevel">
            <text class="memberTitle">点击登录账号</text>
          </view>
        </block>
      </view>
    </view>
  </view>

